{
  "name": "apexlinks",
  "type": "module",
  "private": true,
  "scripts": {
    "a": "concurrently \"npm:server\" \"npm:dev\"",
    "server": "node src/socket",
    "dev": "svelte-kit dev --port 3700",
    "odev": "svelte-kit dev --port 3700 --open",
    "sdev": "svelte-kit dev --https --port 3700",
    "build": "svelte-kit build",
    "export": "svelte-kit export",
    "start": "node build",
    "preview": "svelte-kit preview -H --port 3200"
  },
  "dependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/site-kit": "^2.1.0",
    "cookie": "^0.5.0",
    "fuzzball": "^2.1.2",
    "html-minifier": "^4.0.0",
    "marked": "4.0.14",
    "prism-svelte": "^0.5.0",
    "prismjs": "^1.28.0",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "svelte-preprocess": "^4.10.6"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "carbon-components-svelte": "^0.63.3",
    "carbon-icons-svelte": "^11.0.1",
    "eslint": "^8.14.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "prettier": "^2.6.2",
    "svelte": "^3.47.0"
  }
}
