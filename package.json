{
  "name": "apexlinks",
  "type": "module",
  "private": true,
  "scripts": {
    "a": "concurrently \"npm:server\" \"npm:dev\"",
    "server": "node src/socket",
    "dev": "svelte-kit dev --port 3700",
    "sdev": "svelte-kit dev -H --port 3700",
    "build": "svelte-kit build",
    "export": "svelte-kit export",
    "start": "node build",
    "preview": "svelte-kit preview -H --port 3200"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "dependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/site-kit": "^2.0.3",
    "agent-base": "^6.0.2",
    "concurrently": "^7.0.0",
    "cookie": "^0.4.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fuzzball": "^2.1.2",
    "html-minifier": "^4.0.0",
    "inflection": "^1.13.2",
    "marked": "4.0.12",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "svelte-paystack": "^0.2.2",
    "svelte-preprocess": "^4.10.4"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.16.3",
    "babel-jest": "^27.5.1",
    "carbon-components-svelte": "^0.62.2",
    "carbon-icons-svelte": "^10.45.1",
    "eslint": "^8.11.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "gulp": "^4.0.2",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "svelte": "^3.46.4",
    "svelte-jester": "^2.3.2"
  }
}
